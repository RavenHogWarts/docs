{
  "markdownLinkUpdater.exclude": ["**/node_modules/**"],
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "index.md": "*.md"
  },
  "files.exclude": {
    "**/cache": true
  },
  "markdown.copyFiles.destination": {
    "**/docs/**/*.md": "${documentWorkspaceFolder}/docs/public/images/doc/${fileName}"
  }
}
